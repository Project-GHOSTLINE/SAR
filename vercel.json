{
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "crons": [
    {
      "path": "/api/cron/seo-collect",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/cron/cleanup-sessions",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/aggregate-speed-insights",
      "schedule": "0 3 * * *"
    }
  ],
  "headers": [
    {
      "source": "/performance-diagnostic.html",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        }
      ]
    }
  ]
}
