{
  "name": "sar",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:4000": "next dev -p 4000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "memory:load": "node scripts/auto-load-memory.mjs",
    "memory:init": "node scripts/setup-complete.mjs",
    "test:activity": "node scripts/test-activity-logging.mjs",
    "test:routes": "node scripts/verify-all-routes.js",
    "test:view": "open public/route-test-report.html",
    "recon": "node scripts/deep-recon.js",
    "recon:view": "open public/deep-recon-viewer.html",
    "api:test": "bash postman/run.sh dev",
    "api:test:dev": "bash postman/run.sh dev",
    "api:test:staging": "bash postman/run.sh staging",
    "api:test:prod": "bash postman/run.sh prod",
    "api:test:quick": "newman run postman/collections/SAR-API-Tests.postman_collection.json -e postman/environments/dev.json",
    "api:report": "open postman/reports/latest.html",
    "analyze:site": "cd e2e && npx playwright test site-analyzer --project=chromium",
    "analyze:report": "open test-artifacts/site-analysis/report.html",
    "analyze:quickbooks": "cd e2e && npx playwright test admin-quickbooks-analyzer --project=chromium",
    "analyze:quickbooks-report": "open test-artifacts/quickbooks-analysis/quickbooks-analysis.html"
  },
  "dependencies": {
