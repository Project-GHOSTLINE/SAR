    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.26.2",
    "jose": "^6.1.3",
    "lucide-react": "^0.294.0",
    "next": "^14.2.35",
    "next-auth": "^4.24.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.71.1",
    "web-push": "^3.6.7",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/web-push": "^3.6.4",
    "autoprefixer": "^10.4.23",
    "newman": "^6.2.2",
    "newman-reporter-htmlextra": "^1.23.1",
    "pg": "^8.16.3",
